@article{Lynch2022,
  title = {Unlocking the {{Riddles}} of {{Classical Greek Melodies II}}: {{The Revolution}} of the {{New Music}} in the {{Ashmolean Papyri}} ({{DAGM}} 5–6) and {{Athenaeus}}’ {{Paean}} ({{DAGM}} 20)},
